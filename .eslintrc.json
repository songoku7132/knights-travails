{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-alert": "off",
        "no-plusplus": "off",
        "no-unused-vars": "off",
        "class-methods-use-this": "off",
        "no-use-before-define": "off",
        "no-param-reassign": "off",
        "consistent-return": "off",
        "no-loop-func": "off"
    }
}